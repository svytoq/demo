{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "user",
  "type": "object",
  "additionalProperties": false,
  "required": ["name", "emails"],
  "properties": {
    "name": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    },
    "emails": {
      "type": "string",
      "minLength": 1,
      "maxLength": 100
    }
  }
}